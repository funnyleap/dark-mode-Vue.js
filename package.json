{
  "name": "vuedarkmode",
  "version": "0.0.3",
  "description": "A minimalist dark design system for vue.js",
  "author": "Team <team@vuedarkmode.com>",
  "main": "dist/vuedarkmode.umd.js",
  "module": "dist/vuedarkmode.esm.js",
  "unpkg": "dist/vuedarkmode.min.js",
  "scripts": {
    "dev": "nuxt",
    "dev:lambda": "netlify-lambda serve server/src",
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd",
    "build:lambda": "netlify-lambda build server/src",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vuedarkmode.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vuedarkmode.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vuedarkmode.min.js",
    "generate": "NODE_ENV=production nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "hint.css": "^2.5.0",
    "lodash": "^4.17.10",
    "normalize.css": "^8.0.0",
    "nuxt": "^1.0.0",
    "qs": "^6.5.2"
  },
  "devDependencies": {
    "@vue/eslint-config-prettier": "^3.0.3",
    "autoprefixer": "^9.1.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "dotenv": "^6.1.0",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "minimist": "^1.2.0",
    "netlify-lambda": "^1.0.2",
    "node-sass": "^4.9.3",
    "nuxt-sass-resources-loader": "^2.0.5",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "rollup": "^0.66.6",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^4.3.2",
    "sass-loader": "^7.1.0"
  },
  "files": [
    "components/darkmode/*",
    "dist/*",
    "package.json",
    "package-lock.json"
  ]
}
